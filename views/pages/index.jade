		each floor in floors
			div.floor(id='#{floor.floorId}')
				div.floor-icon
				h4.floor-name=floor.floorName
				h4.floor-sub-name=floor.floorSubName
				div.floor-content
					ul.floor-hot-words
						each word in floor.floorHotwords 
							li 
								a(href='javascript:;')=word
					a.floor-content-left
						img(src='#{floor.MainImg}')
						div.act-title-warp
							div.act-title
								each  slide in floor.secondFloorSlides
									div.slide-item=slide
						div.title-con
							div.title-con-tile #{floor.floorContitle}
							div.title-con-subtitle #{floor.floorSubtitle}
					div.floor-content-middle
						each grid in floor.grids
							a.grid.one-grid
								div.title=grid.title
								div.subTitle=grid.subtitle
								img(src='#{grid.imgsrc}')
					div.floor-content-right
						each aaa in floor.right
							a.grid.two-grid
								div.title=aaa.title
								div.subTitle=aaa.subtitle
								img(src='#{aaa.imgsrc}')
