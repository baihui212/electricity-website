					span.dash.dash-left
					span.glyphicon.glyphicon-heart-empty.txt-icon
					em 猜你喜欢
					span.dash.dash-right
			div.guess-content
				ul.guess-list
					each wonder in wonderful
						li.guess-goods
							a(href='javascript:;').guess-goods-link 
								img(src='#{wonder.imgsrc}')
								em.item-desc=wonder.desc
								span.item-price ￥
